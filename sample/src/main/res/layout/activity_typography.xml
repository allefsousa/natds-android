<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".tokens.SpacingActivity">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="10dp">
            <TextView
                android:id="@+id/h1Sample"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="Heading 1"
                android:textAppearance="?attr/textAppearanceHeadline1"
                android:textColor="?attr/colorHighEmphasis"/>
            <ImageView
                android:layout_height="0.5dp"
                android:layout_width="match_parent"
                android:background="@color/mediumGrey"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="5dp"/>
            <TextView
                android:id="@+id/h2Sample"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="Heading 2"
                android:textAppearance="?attr/textAppearanceHeadline2"
                android:textColor="?attr/colorHighEmphasis"/>
            <ImageView
                android:layout_height="0.5dp"
                android:layout_width="match_parent"
                android:background="@color/mediumGrey"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <TextView
                android:id="@+id/h3Sample"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="Heading 3"
                android:textAppearance="?attr/textAppearanceHeadline3"
                android:textColor="?attr/colorHighEmphasis"/>
            <ImageView
                android:layout_height="0.5dp"
                android:layout_width="match_parent"
                android:background="@color/mediumGrey"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <TextView
                android:id="@+id/h4Sample"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="Heading 4"
                android:textAppearance="?attr/textAppearanceHeadline4"
                android:textColor="?attr/colorHighEmphasis"/>
            <ImageView
                android:layout_height="0.5dp"
                android:layout_width="match_parent"
                android:background="@color/mediumGrey"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <TextView
                android:id="@+id/h5Sample"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="Heading 5"
                android:textAppearance="?attr/textAppearanceHeadline5"
                android:textColor="?attr/colorHighEmphasis"/>
            <ImageView
                android:layout_height="0.5dp"
                android:layout_width="match_parent"
                android:background="@color/mediumGrey"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <TextView
                android:id="@+id/h6Sample"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="Heading 6"
                android:textAppearance="?attr/textAppearanceHeadline6"
                android:textColor="?attr/colorHighEmphasis"/>
            <ImageView
                android:layout_height="0.5dp"
                android:layout_width="match_parent"
                android:background="@color/mediumGrey"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <TextView
                android:id="@+id/subtitle1Sample"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="Subtitle 1"
                android:textAppearance="?attr/textAppearanceSubtitle1"
                android:textColor="?attr/colorHighEmphasis"/>
            <ImageView
                android:layout_height="0.5dp"
                android:layout_width="match_parent"
                android:background="@color/mediumGrey"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <TextView
                android:id="@+id/subtitle2Sample"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="Subtitle 2"
                android:textAppearance="?attr/textAppearanceSubtitle2"
                android:textColor="?attr/colorHighEmphasis"/>
            <ImageView
                android:layout_height="0.5dp"
                android:layout_width="match_parent"
                android:background="@color/mediumGrey"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <TextView
                android:id="@+id/body1Sample"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="Body 1"
                android:textAppearance="?attr/textAppearanceBody1"
                android:textColor="?attr/colorHighEmphasis"/>
            <ImageView
                android:layout_height="0.5dp"
                android:layout_width="match_parent"
                android:background="@color/mediumGrey"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <TextView
                android:id="@+id/body2Sample"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="Body 2"
                android:textAppearance="?attr/textAppearanceBody2"
                android:textColor="?attr/colorHighEmphasis"/>
            <ImageView
                android:layout_height="0.5dp"
                android:layout_width="match_parent"
                android:background="@color/mediumGrey"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <TextView
                android:id="@+id/buttonSample"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="Button"
                android:textAppearance="?attr/textAppearanceButton"
                android:textColor="?attr/colorHighEmphasis"/>
            <ImageView
                android:layout_height="0.5dp"
                android:layout_width="match_parent"
                android:background="@color/mediumGrey"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <TextView
                android:id="@+id/captionSample"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="Caption"
                android:textAppearance="?attr/textAppearanceCaption"
                android:textColor="?attr/colorHighEmphasis"/>
            <ImageView
                android:layout_height="0.5dp"
                android:layout_width="match_parent"
                android:background="@color/mediumGrey"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <TextView
                android:id="@+id/overlineSample"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="Overline"
                android:textAppearance="?attr/textAppearanceOverline"
                android:textColor="?attr/colorHighEmphasis"
                android:layout_marginBottom="20dp"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>