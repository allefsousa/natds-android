<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="notify" format="integer" />
    <attr name="rippleDrawable" format="reference" />
    <attr name="customElevation" format="reference" />
    <attr name="customHeight" format="reference" />
    <attr name="customWidth" format="reference" />

    <declare-styleable name="Tag">
        <attr name="tag_type" format="enum">
            <enum name="primary" value="0" />
            <enum name="alert" value="1" />
            <enum name="secondary" value="2" />
            <enum name="success" value="3" />
            <enum name="warning" value="4" />
            <enum name="link" value="5" />
        </attr>
        <attr name="tag_size" format="enum">
            <enum name="small" value="0" />
            <enum name="standard" value="1" />
        </attr>
        <attr name="colorBackground" />
        <attr name="labelAppearance" />
        <attr name="textLabel" />
        <attr name="customHeight" />
        <attr name="android:textColor" />
    </declare-styleable>

    <declare-styleable name="Shortcut">
        <attr name="type" format="enum">
            <enum name="outlined" value="0" />
            <enum name="contained" value="1" />
        </attr>
        <attr name="colorBackground" />
        <attr name="colorIcon" format="color" />
        <attr name="labelAppearance" format="reference" />
        <attr name="textLabel" format="string" />
        <attr name="iconName" format="string" />
        <attr name="notify" />

    </declare-styleable>

    <declare-styleable name="AppBar">
        <attr name="showLogo" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Badge">
        <attr name="badgeNumber" format="integer" />
        <attr name="badgeVisibility" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ds_menu">
        <attr name="menu_label" format="string" />
        <attr name="menu_label_color" format="reference" />
        <attr name="menu_icon" format="string" />
        <attr name="menu_label_size" format="reference" />
        <attr name="menu_selected_drawable" format="reference" />
        <attr name="menu_opened_drawable" format="reference" />
        <attr name="menu_is_selected" format="boolean" />
        <attr name="menu_is_enabled" format="boolean" />
        <attr name="menu_is_opened" format="boolean" />
        <attr name="menu_has_tag" format="boolean" />
        <attr name="menu_tag_label" format="string" />
    </declare-styleable>

    <declare-styleable name="ds_submenu">
        <attr name="submenu_label" format="string" />
        <attr name="submenu_label_color" format="reference" />
        <attr name="submenu_label_size" format="reference" />
        <attr name="submenu_selected_drawable" format="reference" />
        <attr name="submenu_is_selected" format="boolean" />
        <attr name="submenu_is_enabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ds_primary_button">
        <attr name="button_label" format="string" />
        <attr name="button_icon_left" format="string" />
        <attr name="button_icon_right" format="string" />
        <attr name="button_enabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ds_error_default">
        <attr name="error_icon" format="reference" />
        <attr name="error_label" format="string" />
        <attr name="error_button_label" format="string" />
    </declare-styleable>

    <declare-styleable name="ds_text_input_highlight">
        <attr name="description_label" format="string" />
        <attr name="highlight_label" format="string" />
    </declare-styleable>

    <declare-styleable name="ds_text_field_input">
        <attr name="android:inputType" />
        <attr name="android:maxLines" />
        <attr name="android:maxLength" />
        <attr name="android:lines" />
        <attr name="android:hint" />
        <attr name="android:enabled" />
        <attr name="text_field_text" format="string" />
        <attr name="text_field_icon" format="string" />
        <attr name="text_field_label" format="string" />
        <attr name="text_field_footer" format="string" />
        <attr name="text_field_state" format="enum">
            <enum name="none" value="0" />
            <enum name="success" value="1" />
            <enum name="error" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ds_expansion_panel">
        <attr name="title" format="string" />
    </declare-styleable>

    <declare-styleable name="IconButton">
        <attr name="android:enabled" />
        <attr name="iconName" />
        <attr name="buttonColor" format="enum">
            <enum name="defaultColor" value="0" />
            <enum name="primaryColor" value="1" />
            <enum name="lightColor" value="2" />
        </attr>
        <attr name="sizeButton" format="enum">
            <enum name="semi" value="0" />
            <enum name="semix" value="1" />
            <enum name="medium" value="2" />
        </attr>
        <attr name="styleButton" format="enum">
            <enum name="inherit" value="0" />
            <enum name="floating" value="1" />
            <enum name="overlay" value="2" />
        </attr>
        <attr name="iconColor" format="reference" />
        <attr name="surfaceSize" format="reference" />
        <attr name="customElevation" />
        <attr name="backgroundSize" format="reference" />
        <attr name="backgroundDrawable" format="reference" />
        <attr name="rippleDrawable" />
        <attr name="iconSize" />
        <attr name="notify" />
    </declare-styleable>

    <declare-styleable name="Card">
        <attr name="colorBackground" />
        <attr name="customRadius" format="reference" />
        <attr name="enabledRadius" format="boolean" />
        <attr name="enabledElevation" format="boolean" />
        <attr name="customElevation" />
    </declare-styleable>

    <declare-styleable name="DividerLine">
        <attr name="android:layout_marginLeft" />
        <attr name="android:layout_marginRight" />
        <attr name="colorBackground" />
        <attr name="dividerType" format="enum">
            <enum name="fullBleed" value="0" />
            <enum name="inset" value="1" />
            <enum name="middle" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ProgressIndicator">
        <attr name="customHeight" />
        <attr name="customWidth" />
        <attr name="colorBackground" />
        <attr name="layer" format="boolean" />
        <attr name="size" format="enum">
            <enum name="standard" value="0" />
            <enum name="semi" value="1" />
            <enum name="medium" value="2" />
            <enum name="large" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ListItem">
        <attr name="touchState" format="boolean" />
        <attr name="selectableState" format="boolean" />
        <attr name="dividerBottom" format="enum">
            <enum name="none" value="0" />
            <enum name="fullBleed" value="1" />
            <enum name="inset" value="2" />
            <enum name="middle" value="3" />
        </attr>
        <attr name="rippleDrawable" />
        <attr name="colorBackground" />
    </declare-styleable>

</resources>
